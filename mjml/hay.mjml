<mjml>
  <mj-head>

    <!-- fonts -->
    <mj-font name="Roboto" href="https://fonts.googleapis.com/css?family=Roboto" />
    <mj-font name="Libre Baskerville" href="https://fonts.googleapis.com/css?family=Libre+Baskerville" />



    <mj-attributes>
      <!-- globals -->
      <mj-section background-color="#fcf5e4" />

      <!-- colors -->
      <mj-class name="m-dark" color="#2c2b2a" />
      <mj-class name="gray-1" color="#716a62" />
      <mj-class name="gray-2" color="#44423d" />
      <mj-class name="primary" background-color="#65301e" />

      <!-- elements -->
      <mj-class name="heading-1" mj-class="gray-2" font-size="30px" align="center" font-family="Libre Baskerville" />

      <mj-class name="heading-2" mj-class="gray-2" font-weight="bold" font-size="15px" align="center" font-family="Roboto" />

      <mj-class name="heading-3" mj-class="gray-2" font-weight="semi-bold" font-size="14px" align="center" font-family="Roboto" />

      <mj-class name="follow-text" font-weight="semi-bold" align="center" font-size="16px" line-height="1.4" font-family="Roboto" />

      <mj-class name="slim-img" width="450px" />
      <mj-class name="item-col" background-color="#f7e7c7" padding-bottom="25px" />

    </mj-attributes>


    <mj-style>
      @media only screen and (max-width: 500px) {
      .has__heading-1 div {
      font-size: 24px !important;
      line-height: 1.4 !important;

      }

      .has__heading-2 div {
      font-size: 14px !important;
      line-height: 1.4 !important;


      }
      .has__title div {
      font-size: 28px !important;

      }

      .has__follow__text div {
      font-size: 14px !important;

      }

      .has__items > table > tbody > tr > td {

      padding: 0px 20px 0px 20px !important;
      }

      .has__item__col table tbody tr td table tbody tr td {
      width: 80% !important;
      }

      br{
      display: none;
      }
      }
    </mj-style>

  </mj-head>
  <mj-body background-color="#e4e4e4">

    <mj-section>

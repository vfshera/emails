        <mj-spacer height="25px" />

        <!-- sale -->
        <mj-text css-class="has__title" mj-class="m-dark" padding="0px" align="center" font-size="40px" font-weight="bold">
          THE AUTUMN SALE
        </mj-text>

        <!-- Save -->
        <mj-text mj-class="gray-1" align="center">
          Save 15% sitewide. Plus, enjoy free shipping on accessories
        </mj-text>


        <!-- cta -->
        <mj-button mj-class="primary" font-weight="bold" font-size="15px" inner-padding="16px 25px">
          SHOP THE SALE
